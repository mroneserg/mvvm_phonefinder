<Window x:Class="mvvm_phonefinder.StreetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mvvm_phonefinder.ViewModel" d:DataContext="{d:DesignInstance Type=local:MainViewModel}"
        mc:Ignorable="d"
        Title="StreetWindow" Height="350" Width="500">
    <Grid>
        <DataGrid ItemsSource="{Binding Streets}" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Улицы" Binding="{Binding street}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
